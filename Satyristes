;; For Satyrographos 0.0.2 series
(version 0.0.2)

(library
  (name "xpath")
  (version "0.2.0")
  (sources
    (
     (packageDir "xpath")
     (packageDir "shared")
    )
  )
  (opam "satysfi-xpath.opam")
  (dependencies
    ()
  )
  (compatibility ())
)

(library
  (name "xpath-gr")
  (version "0.2.0")
  (sources
    (
     (packageDir "gr")
     (packageDir "shared")
    )
  )
  (opam "satysfi-xpath-gr.opam")
  (dependencies
    (
      (xpath ())
    )
  )
  (compatibility ())
)

(libraryDoc
  (name "xpath-doc")
  (version "0.2.0")
  (workingDirectory "./doc/")
  (build
    (
      (satysfi "xpath-doc.saty" "-o" "xpath-doc.pdf")
    )
  )
  (sources
    (
      (doc "xpath-doc.pdf" "./doc/xpath-doc.pdf")
    )
  )
  (opam "satysfi-xpath-doc.opam")
  (dependencies
    (
    )
  )
)
